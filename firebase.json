    {
      "source": "/docs/tutorials/crowdfunding",
      "destination": "/docs-tutorials-crowdfunding.html"
    },
    {
      "source": "/docs/tutorials/document-signing",
      "destination": "/docs-tutorials-document-signing.html"
    },
    {
      "source": "/docs/tutorials/filesharing",
      "destination": "/docs-tutorials-filesharing.html"
    },
    {
      "source": "/docs/tutorials/hello-world",
      "destination": "/docs-tutorials-hello-world.html"
    },
    {
      "source": "/docs/tutorials/messaging",
      "destination": "/docs-tutorials-messaging.html"
    },
    {
      "source": "/docs/tutorials/package-manager",
      "destination": "/docs-tutorials-package-manager.html"
    },
    {
      "source": "/docs/tutorials/social-networking",
      "destination": "/docs-tutorials-social-networking.html"
    },
