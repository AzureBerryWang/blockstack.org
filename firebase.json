{
  "hosting": {
    "public": "build",
    "ignore": [
      "**/.*",
      "./__tests__/**",
      "./app/**",
      "./gulp/**",
      "**/node_modules/**",
      "./unused/**",
      "firebase.json"
    ],
    "rewrites": [
      {
        "source": "/docs/articles/404",
        "destination": "/docs-articles-404.html"
      },
      {
        "source": "/docs/articles/blockchain-identity",
        "destination": "/docs-articles-blockchain-identity.html"
      },
      {
        "source": "/docs/articles/blockstack-core",
        "destination": "/docs-articles-blockstack-core.html"
      },
      {
        "source": "/docs/articles/blockstack-profiles",
        "destination": "/docs-articles-blockstack-profiles.html"
      },
      {
        "source": "/docs/articles/blockstack-vs-dns",
        "destination": "/docs-articles-blockstack-vs-dns.html"
      },
      {
        "source": "/docs/articles/blockstack-vs-namecoin",
        "destination": "/docs-articles-blockstack-vs-namecoin.html"
      },
      {
        "source": "/docs/articles/browser-beta",
        "destination": "/docs-articles-browser-beta.html"
      },
      {
        "source": "/docs/articles/faq",
        "destination": "/docs-articles-faq.html"
      },
      {
        "source": "/docs/articles/how-blockstack-works",
        "destination": "/docs-articles-how-blockstack-works.html"
      },
      {
        "source": "/docs/articles/identity-attestation",
        "destination": "/docs-articles-identity-attestation.html"
      },
      {
        "source": "/docs/articles/light-clients",
        "destination": "/docs-articles-light-clients.html"
      },
      {
        "source": "/docs/articles/namespaces",
        "destination": "/docs-articles-namespaces.html"
      },
      {
        "source": "/docs/tutorials/hello-world",
        "destination": "/docs-tutorials-hello-world.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/docs/blockchain-id",
        "destination": "/docs/blockchain-identity",
        "type": 301
      },
      {
        "source": "/docs/what-is-blockstack",
        "destination": "/articles/blockstack-core",
        "type": 301
      },
      {
        "source": "/docs",
        "destination": "/articles",
        "type": 301
      }
    ]
  }
}